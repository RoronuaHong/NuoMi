

{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    [
      "import",
      {
        "libraryName": "antd-mobile",
         style: "css"
      }
    ],
    [
      "transform-class-properties",
      {
        "spec": true
      }
    ],
    [
      "transform-object-rest-spread"
    ]
  ]
}
